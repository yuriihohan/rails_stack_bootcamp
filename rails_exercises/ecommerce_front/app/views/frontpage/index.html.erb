	<!-- Header -->
	<header class="header1">
		<!-- Header desktop -->
		<div class="container-menu-header">
			<div class="topbar">
				<span class="topbar-child1">
					Free shipping for standard order over $100
				</span>
			</div>

			<div class="wrap_header">
				<!-- Logo -->
				<a href="index.html" class="logo">
					<img src="assets/icons/logo.png" alt="IMG-LOGO">
				</a>

				<!-- Menu -->
				<div class="wrap_menu">
					<nav class="menu">
						<ul class="main_menu">
							<li>
								<a href="index.html">Home</a>
							</li>
							<li>
								<a href="product.html">Shop</a>
							</li>
							<li>
								<a href="about.html">About</a>
							</li>
						</ul>
					</nav>
				</div>

				<!-- Header Icon -->
				<div class="header-icons">
					<a href="#" class="header-wrapicon1 dis-block">
						<img src="assets/icons/icon-header.png" class="header-icon1" alt="ICON">
					</a>
				</div>
			</div>
		</div>

	</header>

	<!-- Slide1 -->
	<section class="slide1">
		<div class="wrap-slick1">
			<div class="slick1">
	            <% @slider_images.each do |slider_image| %>
					<%= render partial: "slider_image", locals: { up_header: slider_image.up_header,
					                                                 mid_header: slider_image.mid_header,
					                                                 button_text: slider_image.button_text } %>   
                <% end %>
			</div>
		</div>
	</section>


	<!-- New Product -->
	<section class="newproduct bgwhite p-t-45 p-b-105">
		<div class="container">
			<div class="sec-title p-b-60">
				<h3 class="m-text5 t-center">
					Featured Products
				</h3>
			</div>

			<!-- Slide2 -->
			<div class="wrap-slick2">
				<div class="slick2">
	            <% @feature_products.each do |feature_product| %>
		            <%= render partial: "feature_product", locals: { header: feature_product.header, price: feature_product.price } %>
	            
                <% end %>
				</div>
			</div>
		</div>
	</section>



	<!-- Blog -->
	<section class="blog bgwhite p-t-94 p-b-65">
		<div class="container">
			<div class="sec-title p-b-52">
				<h3 class="m-text5 t-center">
					Inspire
				</h3>
			</div>
            <div class="row">
			    <% @inspiration_blocks.each do |inspiration_block| %>
		            <%= render partial: "inspire_block", locals: { header: inspiration_block.header, text: inspiration_block.text } %>
	            
                <% end %>
			</div>
		</div>
	</section>

	<!-- Shipping -->
	<section class="shipping bgwhite p-t-62 p-b-46">
		<div class="flex-w p-l-15 p-r-15">
            <% @shipping_info_blocks.each do |info_block| %>
		        <%= render partial: "shipping_flex", locals: { header: info_block.header, content: info_block.content } %>
	        
            <% end %>
		</div>
	</section>


